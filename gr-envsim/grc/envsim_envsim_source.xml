<?xml version="1.0"?>
<block>
  <name>envsim_source</name>
  <key>envsim_envsim_source</key>
  <category>[envsim]</category>
  <import>import envsim</import>
  <make>envsim.envsim_source($sample_rate)</make>
  <param>
    <name>Sample Rate</name>
    <key>sample_rate</key>
    <value>samp_rate</value>
    <type>float</type>
  </param>
  <sink>
    <name>packets</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
